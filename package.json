{
	"license": "MIT",
	"scripts": {
		"start": "yarn build && node dist/index.js",
		"build": "tsc",
		"dev": "ts-node-dev -r dotenv/config --respawn --pretty --transpile-only src/index.ts",
		"gen:schema": "awk 1 ./prisma/schemas/*.prisma > ./prisma/schema.prisma && prisma format",
		"prisma:migrate": "prisma migrate dev",
		"prisma:reset": "prisma migrate reset",
		"prisma:generate": " yarn gen:schema && prisma generate"
	},
	"devDependencies": {
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@prisma/client": "^4.1.0",
		"apollo-server-express": "^3.10.0",
		"argon2": "^0.28.7",
		"connect-redis": "^6.1.3",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-session": "^1.17.3",
		"graphql": "^16.5.0",
		"graphql-scalars": "^1.17.0",
		"ioredis": "^5.2.2",
		"nexus": "^1.3.0"
	}
}
